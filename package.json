{
  "name": "muscari-web",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://eighty88.github.io/MuscariWeb/",
  "scripts": {
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:client": "next dev -p 8000",
    "build": "run-p build:client",
    "build:client": "next build && next export",
    "lint": "eslint --ext .ts,.js,.tsx . && prettier --check \"./**/*.ts\"",
    "lint:fix": "eslint --ext .ts,.js,.tsx . --fix && prettier --write \"./**/*.ts\"",
    "start": "run-p start:*",
    "start:client": "next start",
    "test": "jest",
    "export": "next export"
  },
  "dependencies": {
    "@prisma/client": "^2.21.2",
    "axios": "^0.21.1",
    "class-validator": "^0.13.1",
    "next": "^10.1.3",
    "prisma": "^2.21.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.35.2",
    "swr": "^0.5.5"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.6",
    "@types/jest": "^26.0.22",
    "@types/node": "^16.4.0",
    "@types/react": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "dotenv": "^8.6.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-react": "^7.23.2",
    "fastify": "^3.15.0",
    "fastify-cors": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.5",
    "ts-loader": "^8.1.0",
    "ts-node": "^9.1.1",
    "typescript": "~4.2"
  }
}
